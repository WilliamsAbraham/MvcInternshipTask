@model IEnumerable<PosApp.Models.TransactionDetails>

<p>
    @Html.ActionLink("Create New", "CreateTransaction")
</p>
<p>
    @Html.ActionLink("Back", "Index", "Home",null,null)
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.ResponseCode)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Aid)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Rrn)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Ptad)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.ResponseCode)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Aid)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Rrn)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Ptad)
        </td>
        <td>
            @Html.ActionLink("Edit", "EditTransaction", new { id=item.Id }) |
           
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>
